  "mcpServers": {
    "github": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN=${GITHUB_PERSONAL_ACCESS_TOKEN}",
        "ghcr.io/github/github-mcp-server"
      ]
    },
    "dbt": {
      "command": "/Users/devonfulcher/git/dbt-mcp/.venv/bin/mcp",
      "args": [
        "run",
        "/Users/devonfulcher/git/dbt-mcp/src/dbt_mcp/main.py"
      ]
    },
    "Notion": {
      "url": "https://mcp.notion.com/mcp"
    }
  }
}