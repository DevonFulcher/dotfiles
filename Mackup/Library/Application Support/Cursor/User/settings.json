{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.startupEditor": "none",
  "go.formatTool": "gofmt",
  "go.toolsManagement.autoUpdate": true,
  "window.zoomLevel": 1.5,
  "editor.fontSize": 18,
  "editor.fontFamily": "'Monaspace Neon', Menlo, Monaco, 'Courier New', monospace",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "notebook.lineNumbers": "on",
  "security.workspace.trust.untrustedFiles": "open",
  "files.exclude": {
    "**/.git": false
  },
  "redhat.telemetry.enabled": false,
  "[python]": {
    "editor.tabSize": 4,
    "editor.formatOnSaveMode": "file",
    "editor.formatOnPaste": false,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "formatting.provider": "none",
    "testing.pytestEnabled": true,
    "analysis.autoImportCompletions": true,
    "analysis.typeCheckingMode": "basic"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "updateImportsOnFileMove.enabled": "always"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "updateImportsOnFileMove.enabled": "always"
  },
  "files.autoSave": "afterDelay",
  "editor.wordWrap": "on",
  "workbench.editor.enablePreview": false,
  "workbench.editor.empty.hint": "hidden",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingDelete": "never",
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingQuotes": "never",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.stickyScroll.enabled": true,
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "search.useGlobalIgnoreFiles": true,
  "search.exclude": {
    "**/.git": true,
    "**/site-packages/**": true,
    "**/node_modules/*/**": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "githubPullRequests.pullBranch": "never",
  "debug.allowBreakpointsEverywhere": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.inheritEnv": false,
  "security.promptForLocalFileProtocolHandling": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "remote.autoForwardPortsSource": "hybrid",
  "workbench.trustedDomains.promptInTrustedWorkspace": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "editor.accessibilitySupport": "off",
  "git.openRepositoryInParentFolders": "never",
  "makefile.configureOnOpen": false,
  "git.autofetch": true,
  "window.customTitleBarVisibility": "never",
  "window.titleBarStyle": "native",
  "workbench.statusBar.visible": false,
  "breadcrumbs.enabled": true,
  "window.title": "${dirty}${separator}${activeEditorShort}${separator}${activeRepositoryBranchName}${separator}${rootName}${separator}${profileName}",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/subtree-cache/**": true,
    "**/.git/objects/**": true,
    "**/site-packages/**": true,
    "**/node_modules/*/**": true
  },
  "workbench.reduceMotion": "on",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "workbench.colorTheme": "Default High Contrast",
  "indentRainbow.colors": [
    "rgba(255,255,64,0.15)",
    "rgba(127,255,127,0.15)",
    "rgba(255,127,255,0.15)",
    "rgba(79,236,236,0.15)"
  ]
}
