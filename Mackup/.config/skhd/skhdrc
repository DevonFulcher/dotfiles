# move current window left
cmd - u : yabai -m window --grid 1:2:0:0:1:1
# move current window right
cmd - i : yabai -m window --grid 1:2:1:0:1:1
# make current window full screen
cmd - o : yabai -m window --grid 1:1:0:0:1:1

# activate applications
cmd - 1 : osascript -e 'tell application "Cursor" to activate'
cmd - 2 : osascript -e 'tell application "Warp" to activate'
cmd - 3 : osascript -e 'tell application "Chrome" to activate'
cmd - 4 : osascript -e 'tell application "Slack" to activate'
cmd - 5 : osascript -e 'tell application "zoom.us" to activate'
